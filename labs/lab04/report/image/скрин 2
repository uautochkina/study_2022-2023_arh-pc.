cur.actualizeBox.changed = true;

extend(cur, {
  showActualizeValue: function(link) {
    ajax.post('al_settings.php', {act: 'a_get_actualize_value', type: cur.actualizeOptions.type, hash: cur.actualizeOptions.hash}, {
      onDone: function(value) {
        val('actualize_box_inp', value);
        hide(link);
      }
    });
    return false;
  },
  confirmActualData: function(btn) {
    if (buttonLocked(btn)) return;
    lockButton(btn);
    ajax.post('al_settings.php', {act: 'a_confirm_actual', type: cur.actualizeOptions.type, hash: cur.actualizeOptions.hash}, {
      onDone: function(value) {
        unlockButton(btn);
        cur.actualizeBox.hide();
      }
    });
    return false;
  },
  actualizeData: function() {
    if (cur.actualizeOptions.type == 1) {
      cur.actualizeBox.hide();
      showBox('activation.php', {act: 'change_phone_box', hash: cur.actualizeOptions.submit_hash});
    } else if (cur.actualizeOptions.type == 2) {
      var inp = document.querySelector('#actualize_box_inp');
      cur.actualizeValue = inp.value;
      inp.value = '';
      hide('actualize_box_show_lnk', 'actualize_controls');
      show('actualize_new_controls')
      removeClass(inp, 'disabled');
      inp.disabled = false;
      elfocus(inp);
    }
  },
  cancelActualizeData: function() {
    var inp = document.querySelector('#actualize_box_inp');
    inp.value = cur.actualizeValue;
    hide('actualize_new_controls');
    show('actualize_box_show_lnk', 'actualize_controls');
    addClass(inp, 'disabled');
    inp.disabled = true;
  },
  submitNewData: function(btn) {
    if (buttonLocked(btn)) return;

    if (cur.actualizeOptions.type == 2) {
      var email = trim(document.querySelector('#actualize_box_inp').value);
      if (email.length < 5) {
        elfocus('actualize_box_inp');
        return;
      }
      val('layerbox_submit_result', '');
      lockButton(btn);
      var params = {act: 'a_bind_mail', layer: 1, email: email, hash: cur.actualizeOptions.submit_hash};
      ajax.post('al_settings.php', params, {
        onDone: function (msg) {
          unlockButton(btn);
          cur.actualizeBox.hide();
          showDoneBox(msg, {out: 4000, w: 400});
          if (cur.reloadOnMailBind) {
            nav.reload();
          }
        },
        onFail: function(error) {
          unlockButton(btn);
          showMsg('layerbox_submit_result', error.message || error, 'error', true);
          elfocus('actualize_box_inp');
          return true;
        },
      });
    }
  }
